{
  "compilerOptions": {
     "target": "es6",
     "module": "commonjs",
     "outDir": "./build",
     "rootDir": "./src",
     "removeComments": true,
     "strict": false,
     "noImplicitAny": true,
     "esModuleInterop": true,
     "skipLibCheck": true,
     "forceConsistentCasingInFileNames": true
  }
}